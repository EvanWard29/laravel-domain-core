        "illuminate/database": "~5.0",
        "illuminate/container": "~5.0",
        "illuminate/bus": "~5.0",
        "illuminate/log": "~5.0",
        "illuminate/events": "~5.0",
        "netresearch/jsonmapper": "dev-master",
